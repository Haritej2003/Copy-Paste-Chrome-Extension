{
    "manifest_version": 3,
    "name": "Copy Paste Extension",
    "version": "1.0",
    "description": "Allows to copy multiple different lines at a single time .",
    "permissions": [
      "clipboardRead","clipboardWrite"
    ],
    "action": {
      
      "default_icon": {
        "16": "icon2.png",
        "48": "icon2.png",
        "128": "icon2.png"
      }
    },
    "background": {
        "service_worker": "background.js"
      },
    "icons": {
      "16": "icon2.png",
      "48": "icon2.png",
      "128": "icon2.png"
    },
    "commands": {
    "trigger-alert": {
      "suggested_key": {
        "default": "Ctrl+C"
      },
      "description": "Trigger an alert"
    }
  }
  }
  